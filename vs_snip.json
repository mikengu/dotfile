{
"Console Log": {
	"prefix": "clg",
	"body": [
		"console.log($0)"
	],
	"description": "Console Log Shortcut"
	},

"This Props": {
	"prefix": "tp",
	"body": [
		"this.props.$0"
	],
	"description": "this.props Shortcut"
	},

	"What is this Props": {
		"prefix": "what",
		"body": [
			"console.log(this.props$0, 'what is this?')"
		],
		"description": "console.log props"
		},

"PropTypes Function": {
		"prefix": "ptf",
		"body": [
			"PropTypes.func.isRequired"
		],
		"description": "PropTypes Function Shortcut"
		},

"PropTypes Object": {
		"prefix": "pto",
		"body": [
			"PropTypes.object.isRequired"
		],
		"description": "PropTypes Object Shortcut"
		},

"PropTypes String": {
		"prefix": "pts",
		"body": [
			"PropTypes.string.isRequired"
		],
		"description": "PropTypes String Shortcut"
		},

"PropTypes Boolean": {
		"prefix": "ptb",
		"body": [
			"PropTypes.bool.isRequired"
		],
		"description": "PropTypes Boolean Shortcut"
		},

"Style Object": {
		"prefix": "sty",
		"body": [
			"style={{$0}}"
		],
		"description": "React Inline Style"
		},

"Import React": {
	"prefix": "ir",
	"body": [
		"import React from 'react'"
	],
	"description": "Import React"
},

"Import React Component": {
	"prefix": "irc",
	"body": [
		"import React, {Component} from 'react'"
	],
	"description": "Import React and Component"
},

"Import PropTypes": {
	"prefix": "ip",
	"body": [
